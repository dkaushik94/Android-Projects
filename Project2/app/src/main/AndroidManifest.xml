<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.uic.dkaush4.project2">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/header_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.DayNight.DarkActionBar">
        <activity android:name=".Gallery">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ImageLightBox">
            <intent-filter>
                <action android:name="android.intent.action.LIGHTBOX" />
            </intent-filter>
        </activity>
        <activity android:name=".dealerships">
            <intent-filter>
                <action android:name="android.intent.action.DEALERSHIPS" />
            </intent-filter>
        </activity>
    </application>

</manifest>